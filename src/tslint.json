{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "variable-name": false,
    "curly": false,
    "no-inferrable-types": false,
    "object-literal-key-quotes": false
  }
}
